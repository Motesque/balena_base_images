{
    "name": "balena-base-images-raspberrypi3",
    "description": "Builds and published the base images we need for balena deployment",
    "source": {
        "type": "GITHUB",
        "location": "https://github.com/Motesque/balena_base_images.git",
        "gitCloneDepth": 1,
        "gitSubmodulesConfig": {
            "fetchSubmodules": false
        },
        "reportBuildStatus": false,
        "insecureSsl": false
    },
    "secondarySources": [],
    "sourceVersion": "master",
    "secondarySourceVersions": [],
    "artifacts": {
        "type": "NO_ARTIFACTS"
    },
    "secondaryArtifacts":[],
    "cache": {
        "type": "NO_CACHE",
        "location": "",
        "modes": [
            "LOCAL_DOCKER_LAYER_CACHE"
        ]
    },
   "environment": {
        "type": "ARM_CONTAINER",
        "image": "aws/codebuild/amazonlinux2-aarch64-standard:1.0",
        "computeType": "BUILD_GENERAL1_LARGE",
        "environmentVariables": [],
        "privilegedMode": true,
        "imagePullCredentialsType": "CODEBUILD"
    },
    "serviceRole": "arn:aws:iam::473689091316:role/service-role/codebuild-build-service-role",
   "timeoutInMinutes": 60,
    "queuedTimeoutInMinutes": 480,
    "encryptionKey": "arn:aws:kms:eu-central-1:473689091316:alias/aws/s3",
    "tags": [
    ],
    "badgeEnabled": true,
     "logsConfig": {
        "cloudWatchLogs": {
            "status": "ENABLED"
        },
        "s3Logs": {
            "status": "DISABLED",
            "encryptionDisabled": false
        }
    }
}
